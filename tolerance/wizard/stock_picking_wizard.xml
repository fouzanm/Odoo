<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--Form view for Wizard to warn Transfer-->
    <record id="stock_picking_wizard_view_form" model="ir.ui.view">
        <field name="name">stock.picking.wizard.view.form</field>
        <field name="model">stock.picking.wizard</field>
        <field name="arch" type="xml">
        <form string="Warning">
            <field name="message" nolabel="1"/>
            <footer>
                <button name="accept_transfer" string="Accept" type="object" class="oe_highlight"/>
                <button string="Don't Accept" special="cancel" type="object" class="oe_highlight"/>
            </footer>
        </form>
        </field>
    </record>
</odoo>
